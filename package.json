{
	"name": "basketballdiary-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"__comments__": "npm run 명령에서 사용할 스크립트를 정의한다.",
		"serve": "vue-cli-service serve --port 5500",
		"local": "vue-cli-service serve --port 5500 --mode local",
		"local80": "vue-cli-service serve --port 80 --mode local",
		"aws": "vue-cli-service serve --port 5500 --mode production",
		"build": "vue-cli-service build",
		"build-dev": "vue-cli-service build --mode development",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "1.6.2",
		"core-js": "^3.8.3",
		"firebase": "^11.6.0",
		"imps-platform-detect": "^1.0.9",
		"vue": "^2.6.14",
		"vue-cookies": "^1.8.1",
		"vue-router": "^3.5.3",
		"vuetify": "^2.6.4"
	},
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/eslint-parser": "^7.12.16",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-service": "5.0.8",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^8.0.3",
		"sass": "~1.32.0",
		"sass-loader": "^10.0.0",
		"vue-cli-plugin-vuetify": "~2.4.8",
		"vue-template-compiler": "^2.6.14",
		"vuetify-loader": "^1.7.0",
		"webpack": "5.89.0",
		"webpack-cli": "4.9.2"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		},
		"rules": {
			"no-mixed-spaces-and-tabs": 0
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
